
/* 
@font-face styles
*/

@font-face {
    font-family: 'noticia';
    src: url('/admin/ui/fonts/noticiatext-italic-webfont.eot');
    src: url('/admin/ui/fonts/noticiatext-italic-webfont.eot?#iefix') format('embedded-opentype'),
         url('/admin/ui/fonts/noticiatext-italic-webfont.woff') format('woff'),
         url('/admin/ui/fonts/noticiatext-italic-webfont.ttf') format('truetype'),
         url('/admin/ui/fonts/noticiatext-italic-webfont.svg#noticia_textitalic') format('svg');
    font-weight: normal;
    font-style: italic;

}

